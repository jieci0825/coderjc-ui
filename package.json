    "@coderjc-ui/theme-chalk": "workspace:*",
    "@coderjc-ui/utils": "workspace:*",
    "@eslint/js": "^9.4.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@types/node": "^20.13.0",
    "@types/vue": "^2.0.0",
    "@vicons/antd": "^0.12.0",
    "@vicons/fluent": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "async-validator": "^4.2.5",
    "babel-loader": "^9.1.3",
