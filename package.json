  "main": "./lib/coderjc-ui.cjs.js",
  "module": "./es/coderjc-ui.es.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm -C examples dev",
    "dev:docs": "pnpm -C docs dev",
