    "@esbuild-kit/cjs-loader": "^2.4.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/gulp": "^4.0.17",
    "@vue-macros/volar": "^0.21.1",
    "esbuild": "^0.21.5",
    "gulp": "^5.0.0",
